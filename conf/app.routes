# microservice specific routes

POST  /services/:service/features/:feature             uk.gov.hmrc.ratelimitedallowlist.controllers.AllowListController.checkAllowList(service: Service, feature: Feature)

GET /services/:service/features/:feature/configuration            uk.gov.hmrc.ratelimitedallowlist.controllers.AllowListAdminController.get(service: Service, feature: Feature)
PATCH /services/:service/features/:feature/configuration          uk.gov.hmrc.ratelimitedallowlist.controllers.AllowListAdminController.patch(service: Service, feature: Feature)
POST /services/:service/features/:feature/configuration/tokens    uk.gov.hmrc.ratelimitedallowlist.controllers.AllowListAdminController.addTokens(service: Service, feature: Feature)
